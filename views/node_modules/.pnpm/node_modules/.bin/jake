#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/DATOS/Desktop/BYU-Idaho/cse 340 - Web Backend Development/Week1/views/node_modules/.pnpm/jake@10.8.7/node_modules/jake/bin/node_modules:/mnt/e/DATOS/Desktop/BYU-Idaho/cse 340 - Web Backend Development/Week1/views/node_modules/.pnpm/jake@10.8.7/node_modules/jake/node_modules:/mnt/e/DATOS/Desktop/BYU-Idaho/cse 340 - Web Backend Development/Week1/views/node_modules/.pnpm/jake@10.8.7/node_modules:/mnt/e/DATOS/Desktop/BYU-Idaho/cse 340 - Web Backend Development/Week1/views/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/DATOS/Desktop/BYU-Idaho/cse 340 - Web Backend Development/Week1/views/node_modules/.pnpm/jake@10.8.7/node_modules/jake/bin/node_modules:/mnt/e/DATOS/Desktop/BYU-Idaho/cse 340 - Web Backend Development/Week1/views/node_modules/.pnpm/jake@10.8.7/node_modules/jake/node_modules:/mnt/e/DATOS/Desktop/BYU-Idaho/cse 340 - Web Backend Development/Week1/views/node_modules/.pnpm/jake@10.8.7/node_modules:/mnt/e/DATOS/Desktop/BYU-Idaho/cse 340 - Web Backend Development/Week1/views/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jake/bin/cli.js" "$@"
else
  exec node  "$basedir/../jake/bin/cli.js" "$@"
fi
